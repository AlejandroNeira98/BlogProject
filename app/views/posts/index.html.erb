<h1> Posts list </h1>
<%= render partial: "shared/usercard", locals: { user: @user} %>
<% @posts.each do |post|%>
  <%= render partial: "shared/postcard", locals: { post: post} %>
  <%= render partial: "shared/commentscard", locals: {comments: @comments.where(post_id: post.post_id).limit(5)}%>
<% end %>